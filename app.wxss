/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.all {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  max-width: 750rpx;
  overflow-x: hidden;
}

.rect {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
  position: fixed;
  top: 0;
  z-index: 110;
}

.bold {
  font-weight: bold;
}

.Eleven {
  font-size: 21rpx;
}

.Twelve {
  font-size: 23rpx;
}

.Thirteen {
  font-size: 25rpx;
}

.Fourteen {
  font-size: 27rpx;
}

.Fifteen {
  font-size: 29rpx;
}

.Seventeen {
  font-size: 30rpx;
}

.Twenty {
  font-size: 36rpx;
}

.Twenty-five {
  font-size: 46rpx;
}

.relative {
  position: relative;
}

.contact {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  opacity: 0;
}

.column{
  display: flex;
  flex-direction: column;
}

.bottom_none {
  margin-bottom: 0rpx !important;
}

.white {
  color: white;
}

.yellow {
  color: #ffcd11;
}

.gray {
  color: #999;
}

.red {
  color: #f11;
}
